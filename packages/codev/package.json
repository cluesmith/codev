{
  "name": "@cluesmith/codev",
  "version": "2.0.0-rc.26",
  "description": "Codev CLI - AI-assisted software development framework",
  "type": "module",
  "bin": {
    "codev": "./bin/codev.js",
    "porch": "./bin/porch.js",
    "af": "./bin/af.js",
    "consult": "./bin/consult.js",
    "generate-image": "./bin/generate-image.js"
  },
  "files": [
    "dist",
    "bin",
    "skeleton",
    "templates"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc && npm run copy-skeleton",
    "copy-skeleton": "rm -rf skeleton && cp -r ../../codev-skeleton skeleton",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "test": "vitest",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:e2e:watch": "vitest --config vitest.e2e.config.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@google/genai": "^1.0.0",
    "better-sqlite3": "^12.5.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "glob": "^11.0.0",
    "http-proxy": "^1.18.1",
    "js-yaml": "^4.1.0",
    "open": "^10.1.0",
    "qrcode-terminal": "^0.12.0",
    "tree-kill": "^1.2.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/http-proxy": "^1.17.17",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.1",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/ws": "^8.18.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "cli",
    "codev",
    "ai",
    "development",
    "orchestration",
    "agent-farm",
    "multi-agent"
  ],
  "author": "Cluesmith",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cluesmith/codev"
  }
}
