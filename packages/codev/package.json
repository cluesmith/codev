{
  "name": "@cluesmith/codev",
  "version": "2.0.0-rc.69",
  "description": "Codev CLI - AI-assisted software development framework",
  "type": "module",
  "bin": {
    "codev": "./bin/codev.js",
    "porch": "./bin/porch.js",
    "af": "./bin/af.js",
    "consult": "./bin/consult.js",
    "generate-image": "./bin/generate-image.js"
  },
  "files": [
    "dist",
    "bin",
    "skeleton",
    "templates",
    "dashboard/dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc && npm run build:dashboard && npm run copy-skeleton",
    "build:dashboard": "cd dashboard && npm install && npm run build",
    "dev:dashboard": "cd dashboard && npm run dev",
    "copy-skeleton": "rm -rf skeleton && cp -r ../../codev-skeleton skeleton",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "test": "vitest",
    "test:e2e": "npm run build && vitest run --config vitest.e2e.config.ts",
    "test:e2e:watch": "vitest --config vitest.e2e.config.ts",
    "test:e2e:playwright": "npx playwright test",
    "test:e2e:cli": "npm run build && vitest run --config vitest.cli.config.ts",
    "postinstall": "node -e \"try{require('fs').chmodSync(require('path').join(require.resolve('node-pty'),'..','..','prebuilds',process.platform+'-'+process.arch,'spawn-helper'),0o755)}catch{}\"",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.41",
    "@google/genai": "^1.0.0",
    "better-sqlite3": "^12.5.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "glob": "^11.0.0",
    "js-yaml": "^4.1.0",
    "node-pty": "^1.1.0",
    "open": "^10.1.0",
    "tree-kill": "^1.2.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.1",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^4.0.18",
    "playwright": "^1.58.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "cli",
    "codev",
    "ai",
    "development",
    "orchestration",
    "agent-farm",
    "multi-agent"
  ],
  "author": "Cluesmith",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cluesmith/codev"
  }
}
